{
  "name": "web-tetris-frontend",
  "version": "0.1.0",
  "description": "Easily implement a game of tetris anywhere on the web",
  "main": "src/index.ts",
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js --watch",
