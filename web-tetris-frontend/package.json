  },
  "homepage": "https://github.com/rustwasm/create-wasm-app#readme",
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "hello-wasm-pack": "^0.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.0",
    "sass-loader": "^13.2.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
